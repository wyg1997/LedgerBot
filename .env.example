# 飞书应用配置
FEISHU_APP_ID=your_app_id
FEISHU_APP_SECRET=your_app_secret

# 飞书多维表格URL，只需这一个！
# 格式：https://example.feishu.cn/base/APP_TOKEN?table=TABLE_TOKEN
FEISHU_BITABLE_URL=https://your-domain.feishu.cn/base/YOUR_APP_TOKEN?table=YOUR_TABLE_TOKEN

# 字段映射配置（根据你的表格字段名设置）
FEISHU_FIELD_DESCRIPTION=描述
FEISHU_FIELD_AMOUNT=金额
FEISHU_FIELD_TYPE=类型
FEISHU_FIELD_CATEGORY=分类
FEISHU_FIELD_DATE=日期
FEISHU_FIELD_USER_NAME=用户
FEISHU_FIELD_ORIGINAL_MSG=原始消息

# AI配置
AI_BASE_URL=https://api.openai.com
AI_API_KEY=your_openai_api_key
AI_MODEL=gpt-3.5-turbo

# 服务器配置
SERVER_PORT=8080
SERVER_READ_TIMEOUT=30
SERVER_WRITE_TIMEOUT=30

# 用户映射文件路径
USER_MAPPING_FILE=./data/user_mapping.json
DATA_DIR=./data
LOG_LEVEL=info

# 缓存配置
CACHE_TTL=3600
CACHE_CLEANUP=300